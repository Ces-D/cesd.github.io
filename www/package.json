{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "www",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "scss-types:build": "typed-scss-modules src -n all --outputFolder __generated__",
    "scss-types:watch": "typed-scss-modules src -w -n all --outputFolder __generated__",
    "start": "vite",
    "lint:fix": "eslint --fix",
    "pretty:fix": "prettier ./src/ --write",
    "dev": "vite",
    "build": "pnpm run scss-types:build && pnpm run lint:fix && vite build",
    "serve": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.30",
    "eslint": "^8.57.0",
    "eslint-plugin-solid": "^0.13.2",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "solid-devtools": "^0.29.3",
    "typed-scss-modules": "^8.0.1",
    "typescript": "^5.4.3",
    "vite": "^5.2.6",
    "vite-plugin-solid": "^2.10.2"
  },
  "dependencies": {
    "@fastify/helmet": "^11.1.1",
    "@solidjs/meta": "^0.29.3",
    "@solidjs/router": "^0.13.1",
    "eslint-plugin-import": "^2.29.1",
    "fastify": "^4.26.2",
    "fuse.js": "^7.0.0",
    "rehype-highlight": "^7.0.0",
    "remark-gfm": "3.0.1",
    "rxjs": "^7.8.1",
    "solid-heroicons": "^3.2.4",
    "solid-js": "^1.8.16",
    "solid-markdown": "^2.0.1"
  },
  "eslintConfig": {
    "files": [
      "**/*.{ts,tsx}"
    ],
    "plugins": [
      "solid",
      "import"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:solid/recommended"
    ],
    "rules": {
      "import/order": [
        "error",
        {
          "pathGroupsExcludedImportTypes": [
            "builtin"
          ],
          "groups": [
            [
              "builtin",
              "external"
            ],
            "internal",
            [
              "parent",
              "sibling",
              "index"
            ]
          ],
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          },
          "warnOnUnassignedImports": true
        }
      ]
    }
  },
  "prettier": {
    "printWidth": 88
  },
  "packageManager": "pnpm@8.8.0+sha256.d713a5750e41c3660d1e090608c7f607ad00d1dd5ba9b6552b5f390bf37924e9"
}
